        "        summarise(sum(Matriculaciones))\n",
        "\n",
        "Matriculaciones_Rama_Grado <- cbind(Matriculaciones_RH_Grado, Matriculaciones_RM_Grado[2])\n",
        "names(Matriculaciones_Rama_Grado) <- c(\"Rama de eneseñanza\", \"Hombres\", \"Mujeres\")\n"
