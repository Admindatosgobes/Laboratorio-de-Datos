    {
      "cell_type": "code",
      "source": [
        "# Ahora realizamos la agregación por rama de enseñanza fitrando por curso, formación y sexo en la tabla Matriculaciones_edad\n",
        "\n",
        "Matriculaciones_RH_Grado <- Matriculaciones_edad %>%\n",
        "    group_by(`Rama de enseñanza`)%>%\n",
        "      filter(Curso == \"2020-2021\", Formacion==\"Grado\", Sexo==\"Hombres\") %>%\n",
        "        summarise(sum(Matriculaciones))\n",
        "\n",
        "Matriculaciones_RM_Grado <- Matriculaciones_edad %>%\n",
